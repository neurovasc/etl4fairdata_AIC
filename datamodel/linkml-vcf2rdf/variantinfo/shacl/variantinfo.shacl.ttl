@prefix faldo: <http://biohackathon.org/resource/faldo#> .
@prefix geno: <http://purl.obolibrary.org/obo/GENO_> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs1: <https://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix so: <http://purl.obolibrary.org/obo/SO_> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://ican.univ-nantes.io/variants-kg/:Container> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class so:0001059 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <https://ican.univ-nantes.io/variants-kg/:variantcatalog> ] ;
    sh:targetClass <https://ican.univ-nantes.io/variants-kg/:Container> .

faldo:Region a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents the location of a sequence alteration." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class faldo:ExactPosition ;
            sh:description "The beginning of the location of the sequence alteration." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path faldo:begin ],
        [ sh:class so:0000353 ;
            sh:description "The reference sequence (contig, sequence, chromosome)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path faldo:reference ],
        [ sh:class faldo:ExactPosition ;
            sh:description "The end of the location of the sequence alteration." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path faldo:end ] ;
    sh:targetClass faldo:Region .

geno:0000002 a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents the alternate allele (geno:0000002)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The value of the alternate allele." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sio:000300 ] ;
    sh:targetClass geno:0000002 .

geno:0000036 a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents the reference allele (geno:0000036)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The value of the reference allele." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sio:000300 ] ;
    sh:targetClass geno:0000036 .

geno:0000133 a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents the zygosity of an associated characteristic." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://ican.univ-nantes.io/variants-kg/:Count> ;
            sh:description "Count of alternative alleles in individuals with a certain zygosity." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path sio:000216 ],
        [ sh:class <https://ican.univ-nantes.io/variants-kg/:Frequency> ;
            sh:description "Frequency of the allele in indibiduals with a certain zygosity." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path sio:000900 ],
        [ sh:description "The type of zygosity." ;
            sh:in ( geno:0000136 geno:0000135 geno:0000391 ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path rdfs1:type ] ;
    sh:targetClass geno:0000133 .

so:0000353 a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents the reference sequence (contig, sequence, chromosome)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The value of the reference sequence (ena sequence i.e. https://www.ebi.ac.uk/ena/browser/view/CM000684.2)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://ican.univ-nantes.io/variants-kg/:value> ],
        [ sh:datatype xsd:string ;
            sh:description "A human-readable label for the reference sequence." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path rdfs1:label ],
        [ sh:datatype xsd:string ;
            sh:description "The value of the reference sequence in another database (ncbi sequence i.e. https://www.ncbi.nlm.nih.gov/nuccore/NC_000022.11)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path owl:sameAs ] ;
    sh:targetClass so:0000353 .

so:0001059 a sh:NodeShape ;
    sh:closed true ;
    sh:description "A representation of a sequence alteration (so:0001059)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class geno:0000036 ;
            sh:description "Links the sequence alteration to its reference allele (geno:0000036) using the property geno:0000385." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path geno:0000385 ],
        [ sh:class geno:0000002 ;
            sh:description "Links the sequence alteration to its alternate allele (geno:0000002) using the property geno:0000382." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path geno:0000382 ],
        [ sh:datatype xsd:string ;
            sh:description "A unique identifier for the sequence alteration." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sio:000300 ],
        [ sh:class <https://ican.univ-nantes.io/variants-kg/:AssociatedCharacteristic> ;
            sh:description "Links the sequence alteration to a phenotype (female), clinical trait (diabetes) or grouping (whole cohort)." ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path sio:001403 ],
        [ sh:class faldo:Region ;
            sh:description "Links the sequence alteration to its location using the property faldo:Region." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path faldo:location ] ;
    sh:targetClass so:0001059 .

<https://ican.univ-nantes.io/variants-kg/:AssociatedCharacteristic> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents a phenotype, clinical trait or grouping associated with a sequence alteration." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class geno:0000133 ;
            sh:description "The zygocity of the associated characteristic." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path geno:0000608 ],
        [ sh:datatype xsd:string ;
            sh:description "A human-readable label for the associated characteristic." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path rdfs1:label ],
        [ sh:datatype xsd:string ;
            sh:description "A unique identifier for the associated characteristic." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sio:000300 ] ;
    sh:targetClass <https://ican.univ-nantes.io/variants-kg/:AssociatedCharacteristic> .

<https://ican.univ-nantes.io/variants-kg/:Count> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents the count of alternative alleles in individuals with a certain zygosity." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "The count value." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sio:000300 ] ;
    sh:targetClass <https://ican.univ-nantes.io/variants-kg/:Count> .

<https://ican.univ-nantes.io/variants-kg/:Frequency> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents the frequency of an allele in individuals with a certain zygosity." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:float ;
            sh:description "The frequency value." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sio:000300 ] ;
    sh:targetClass <https://ican.univ-nantes.io/variants-kg/:Frequency> .

faldo:ExactPosition a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents the beginning of the location of a sequence alteration.",
        "Represents the end of the location of a sequence alteration." ;
    sh:ignoredProperties ( rdf:type ),
        ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "The position of the end of the location of the sequence alteration." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path faldo:position ],
        [ sh:datatype xsd:integer ;
            sh:description "The position of the beginning of the location of the sequence alteration." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path faldo:position ] ;
    sh:targetClass faldo:ExactPosition .

